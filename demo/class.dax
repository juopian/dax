class Doughnut {
  cook() {
    print "Fry until golden brown.";
  }
}

class BostonCream extends Doughnut {
  cook() {
    super.cook();
    print "Pipe full of custard and coat with chocolate.";
  }
}

class NamedClass {
  NamedClass(x, {y}) {
    this.x = x;
    this.y = y;
  }

  show() {
    print "x: ${this.x}, y: ${this.y}";
  }
}

BostonCream().cook();
NamedClass("hello").show();